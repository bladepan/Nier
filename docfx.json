{
  "metadata": [
    {
      "src": [
        {
          "files": [ "Nier.Commons/**/*.cs", "Nier.TwoB/**/*.cs" ],
          "exclude": [ "**/bin/**", "**/obj/**" ]
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
        "src": "obj/api",
        "dest": "api"
      },
      {
        "files": [ "*.md", "toc.yml" ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Nier.Commons",
      "_enableSearch": true
    },
    "markdownEngineName": "markdig",
    "dest": "_site"
  }
}
